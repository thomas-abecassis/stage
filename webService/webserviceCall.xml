 <!-- enregistrer un lot -->
<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" >
   <soapenv:Header>
		<authentification>
			<login>test</login>
			<password>toast</password>
		</authentification>
    </soapenv:Header>
	<soapenv:Body>
	<creerLot>
      <idLot>test24</idLot>
      <ville>Montpellier</ville>
      <surface>11</surface>
      <loyer>11</loyer>
      <typeDeBien>maison</typeDeBien>
      <nombrePiece>2</nombrePiece>
      <description>2</description>
      <informationsCommercial>2</informationsCommercial>
      <plus>
      	<plus1>
            <categorie>Type(s) de pièces</categorie>
            <valeur>salle de bain</valeur>
         </plus1>
      	<plus2>
            <categorie>Commodité(s)</categorie>
            <valeur>alarme</valeur>  
         </plus2>
      </plus>
	</creerLot>
	</soapenv:Body>
</soapenv:Envelope>

 <!-- mettre à jour un lot -->

<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" >
   <soapenv:Header>
    <authentification>
      <login>test</login>
      <password>toast</password>
    </authentification>
    </soapenv:Header>
  <soapenv:Body>
  <mettreAJourLot>
      <idLot>test</idLot>
      <ville>Le Crès</ville>
      <surface>111</surface>
      <loyer>111</loyer>
      <typeDeBien>maison</typeDeBien>
      <nombrePiece>2</nombrePiece>
      <description>2</description>
      <informationsCommercial>2</informationsCommercial>
      <plus>
        <plus1>
            <categorie>Type(s) de pièces</categorie>
            <valeur>salle de bain</valeur>
         </plus1>
        <plus2>
            <categorie>Commodité(s)</categorie>
            <valeur>alarme</valeur>  
         </plus2>
      </plus>
  </mettreAJourLot>
  </soapenv:Body>
</soapenv:Envelope>

<!-- enregistrer une categorie et une valeur (si la categorie n'existe pas elle est créée) -->

