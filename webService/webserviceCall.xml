<!-- Pour supprimer tout les lots -->
<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" >
   <soapenv:Header>
		<authentification>
			<login>test</login>
			<password>toast</password>
		</authentification>
    </soapenv:Header>
	<soapenv:Body>
	<supprimerLots></supprimerLots>
	</soapenv:Body>
</soapenv:Envelope>

<!-- supprimer un seul lot -->

<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" >
   <soapenv:Header>
      <authentification>
         <login>test</login>
         <password>toast</password>
      </authentification>
    </soapenv:Header>
   <soapenv:Body>
   <supprimerUnLot>
      <idLot>1234</idLot>
   </supprimerUnLot>
   </soapenv:Body>
</soapenv:Envelope>

