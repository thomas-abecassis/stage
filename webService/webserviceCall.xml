 <!-- enregistrer une image -->
<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" >
   <soapenv:Header>
		<authentification>
			<login>test</login>
			<password>toast</password>
		</authentification>
    </soapenv:Header>
	<soapenv:Body>
	<saveImage>
		<idLot>58008</idLot>
		<imageString>Ym9uam91cg==</imageString>
	</saveImage>
	</soapenv:Body>
</soapenv:Envelope>

 <!-- enregistrer un lot -->
<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" >
   <soapenv:Header>
		<authentification>
			<login>test</login>
			<password>toast</password>
		</authentification>
    </soapenv:Header>
	<soapenv:Body>
	<creerLot>
      <idLot>tesasdft</idLot>
      <ville>Montpellier</ville>
      <surface>11</surface>
      <loyer>11</loyer>
      <typeDeBien>maison</typeDeBien>
      <nombrePiece>2</nombrePiece>
      <description>2</description>
      <informationsCommercial>2</informationsCommercial>
      <plus>
      	<plus1>
            <categorie>Type(s) de bien</categorie>
            <valeur>appartement</valeur>
         </plus1>
      	<plus2>
            <categorie>Nombre de pièces</categorie>
            <valeur>1</valeur>  
         </plus2>
      </plus>
	</creerLot>
	</soapenv:Body>
</soapenv:Envelope>

<!-- enregistrer une categorie et une valeur (si la categorie n'existe pas elle est créée) -->

<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" >
   <soapenv:Header>
      <authentification>
         <login>test</login>
         <password>toast</password>
      </authentification>
    </soapenv:Header>
   <soapenv:Body>
   <saveCategorieEtValeur>
      <categorie>Option(s)</categorie>
      <valeur>ceci est une option</valeur>
   </saveCategorieEtValeur>
   </soapenv:Body>
</soapenv:Envelope>

<!-- recevoir la liste de toutes les alertes actives enregistrées par les utilisateurs -->

<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" >
   <soapenv:Header>
      <authentification>
         <login>test</login>
         <password>toast</password>
      </authentification>
    </soapenv:Header>
   <soapenv:Body>
   <getAllAlertesActive></getAllAlertesActive>
   </soapenv:Body>
</soapenv:Envelope>