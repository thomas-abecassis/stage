      <authentification>
         <login>test</login>
         <password>toast</password>
      </authentification>
    </soapenv:Header>
   <soapenv:Body>
   <saveCategorieEtValeur>
      <categorie>Option(s)</categorie>
      <valeur>ceci est une option</valeur>
   </saveCategorieEtValeur>
   </soapenv:Body>
</soapenv:Envelope>

